            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.socialmediasaver.status.activity.AboutUsActivity"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.socialmediasaver.status.activity.FacebookActivity"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.socialmediasaver.status.activity.FacebookActivity2"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.socialmediasaver.status.activity.WhatsappActivity"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
