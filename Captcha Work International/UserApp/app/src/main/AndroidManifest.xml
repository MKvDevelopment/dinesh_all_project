<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.work_int.athome">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CaptchaWork">
        <activity
            android:name="com.work_int.athome.VideoActivity"
            android:screenOrientation="landscape"
            android:exported="false" />
        <activity
            android:name="com.work_int.athome.PlanActivity"
            android:exported="false" />
        <activity
            android:name="com.work_int.athome.SpinActivity"
            android:exported="false" />
        <activity
            android:name="com.work_int.athome.OTPMatchActivity"
            android:exported="false" />
        <activity
            android:name="com.work_int.athome.AdsTaskActivity"
            android:exported="false" />
        <activity
            android:name="com.work_int.athome.ContentActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name="com.work_int.athome.WithdrawActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name="com.work_int.athome.LoginActivity"
            android:screenOrientation="nosensor"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name="com.work_int.athome.MainActivity"
            android:theme="@style/Theme.Design.Light.NoActionBar"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name="com.work_int.athome.SplashActivity"
            android:exported="true"
            android:screenOrientation="nosensor"
            android:theme="@style/Theme.Design.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <uses-permission android:name="android.permission.Ad_ID" />


</manifest>